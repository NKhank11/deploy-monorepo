{
  "name": "my-turbo-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2",
    "@types/node": "^20.11.17"
  },
  "engines": {
    "node": ">=18"
  },
  "resolutions": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.0",
    "@types/node": "^20.11.17",
    "ts-node": "^10.9.2"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.0",
    "@types/node": "^20.11.17",
    "ts-node": "^10.9.2"
  },
  "packageManager": "npm@11.5.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}